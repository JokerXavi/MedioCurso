void setup(){
  size(300,300);
}
void draw(){
  
  float var1 =random(0,255);
  float var2 =random(0,255);
  float var3 =random(0,255);
  float var4 =random(0,255);
ellipseMode(CENTER);
fill(var1);
ellipse(50,50,75,75);
fill(var2);
ellipse(250,250,75,75);
fill(var3);
ellipse(50,250,75,75);
fill(var4);
ellipse(250,50,75,75);
}
